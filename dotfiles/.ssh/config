Include ~/.ssh/config.d/identities

Host *
  CheckHostIP yes
  ControlMaster auto
  ControlPath ~/.ssh/cm_socket/%r@%h:%p
    ControlPersist 5s

Host lish-us-east.linode.com
  IdentityFile ~/.ssh/id_ed25519

Host gerrit.fastlizard4.org
  IdentityFile ~/.ssh/id_ed25519

Host phazon.fastlizard4.org
  IdentityFile ~/.ssh/id_ed25519
